{
  "name": "instagram-scraper-bot",
  "version": "1.0.0",
  "description": "Instagram Scraper Bot module for Telegram bot",
  "type": "module",
  "scripts": {
    "dev": "bun run src/bot.ts",
    "start": "bun run src/bot.ts",
    "refresh:viral": "bun run src/scripts/refresh-strategy.ts viral",
    "refresh:full": "bun run src/scripts/refresh-strategy.ts",
    "diagnose:openai": "bun run scripts/test-openai-setup.ts",
    "test:complete": "bun run scripts/one-reel-complete-test.ts",
    "typecheck": "bun --bun exec tsc --noEmit",
    "db:studio": "bunx drizzle-kit studio --port 3457",
    "db:generate": "bunx drizzle-kit generate",
    "db:push": "bunx drizzle-kit push",
    "db:migrate": "bunx drizzle-kit migrate",
    "meta-muse:init": "bun run src/scripts/meta-muse-init-project.ts",
    "meta-muse:scrape": "bun run src/scripts/meta-muse-robust-cycle.ts",
    "meta-muse:check": "bun run src/scripts/meta-muse-check-data.ts",
    "meta-muse:demo": "bun run src/scripts/meta-muse-demo-cycle.ts",
    "meta-muse:quick-test": "bun run src/scripts/meta-muse-quick-test.ts",
    "meta-muse:daily": "bun run src/scripts/meta-muse-daily-runner.ts",
    "sync-obsidian": "bun run src/scripts/sync-obsidian-vault.ts"
  },
  "keywords": [
    "instagram",
    "scraper",
    "telegram",
    "bot"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@notionhq/client": "^3.0.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express-rate-limit": "^6.0.2",
    "@types/helmet": "^4.0.0",
    "apify-client": "^2.12.3",
    "assemblyai": "^4.12.2",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.10.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.43.1",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "open": "^10.1.2",
    "openai": "^4.102.0",
    "pg": "^8.15.6",
    "telegraf": "^4.16.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.2",
    "zod": "^3.24.4"
