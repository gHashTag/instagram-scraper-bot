    "@types/bun": "latest",
    "@types/express": "^5.0.1",
    "@types/node": "^22.5.4",
    "@vercel/node": "^5.2.0",
    "ai": "^4.3.16",
    "dotenv-cli": "^8.0.0",
    "input": "^1.0.1",
    "telegram": "^2.19.10",
    "tsx": "^4.19.4"
  }
